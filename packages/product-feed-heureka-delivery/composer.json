{
    "name": "shopsys/product-feed-heureka-delivery",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Heureka delivery XML product feed generation",
    "keywords": ["Heureka", "product delivery feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaDeliveryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaDeliveryBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "cweagans/composer-patches": "^1.6",
        "doctrine/orm": "2.6.2",
        "shopsys/form-types-bundle": "dev-ss-composer-patches",
        "shopsys/framework": "dev-ss-composer-patches",
        "shopsys/migrations": "dev-ss-composer-patches",
        "shopsys/plugin-interface": "dev-ss-composer-patches",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-kernel": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "shopsys/coding-standards": "dev-ss-composer-patches",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8"
    },
    "conflict": {
        "symfony/dependency-injection": ">=3.4.15"
    },
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Hot fix: https://github.com/doctrine/doctrine2/issues/4869 and DDC-1960": "https://github.com/doctrine/doctrine2/compare/v2.6.2...shopsys:proposal-ddc-1960-ddc-4005.patch"
            }
        }
    }
}
