{
    "name": "shopsys/product-feed-heureka",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Heureka XML product feed generation",
    "keywords": ["Heureka", "product feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "cweagans/composer-patches": "^1.6",
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "2.6.2",
        "shopsys/form-types-bundle": "dev-master",
        "shopsys/framework": "dev-master",
        "shopsys/migrations": "dev-master",
        "shopsys/plugin-interface": "dev-master",
        "symfony/symfony": "^3.4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "shopsys/coding-standards": "dev-master",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8"
    },
    "conflict": {
        "symfony/dependency-injection": ">=3.4.15"
    },
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Hot fix: https://github.com/doctrine/doctrine2/issues/4869 and DDC-1960": "https://github.com/doctrine/doctrine2/compare/v2.6.2...shopsys:proposal-ddc-1960-ddc-4005.patch"
            }
        }
    }
}
